{
    "schemes": [
        "http",
        "https"
    ],
    "swagger": "2.0",
    "info": {
        "description": "A Go service library with HTTP API endpoints",
        "title": "Miko Service API",
        "termsOfService": "http://swagger.io/terms/",
        "contact": {
            "name": "API Support",
            "url": "http://www.swagger.io/support",
            "email": "support@swagger.io"
        },
        "license": {
            "name": "MIT",
            "url": "http://opensource.org/licenses/MIT"
        },
        "version": "1.0"
    },
    "basePath": "/api",
    "paths": {
        "/download": {
            "get": {
                "description": "Download single or multiple music tracks by providing song IDs, album URLs, playlist URLs, or song URLs. Supports batch downloads with various quality levels and conflict resolution policies.",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "music"
                ],
                "summary": "Download music tracks",
                "parameters": [
                    {
                        "type": "array",
                        "items": {
                            "type": "string"
                        },
                        "collectionFormat": "csv",
                        "example": "\"2161154646\"",
                        "description": "Resource URIs to download (can be song ID, album URL, playlist URL, song URL). Multiple URIs supported for batch downloads.",
                        "name": "uri",
                        "in": "query",
                        "required": true
                    },
                    {
                        "enum": [
                            "standard",
                            "higher",
                            "exhigh",
                            "lossless",
                            "hires",
                            "128",
                            "192",
                            "320",
                            "HQ",
                            "SQ",
                            "HR"
                        ],
                        "type": "string",
                        "default": "lossless",
                        "example": "\"hires\"",
                        "description": "Audio quality level",
                        "name": "level",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "example": "\"./downloads\"",
                        "description": "Output directory path for downloaded files",
                        "name": "output",
                        "in": "query"
                    },
                    {
                        "type": "integer",
                        "default": 60000,
                        "example": 60000,
                        "description": "Timeout in milliseconds for the download operation, 0 means no timeout",
                        "name": "timeout",
                        "in": "query"
                    },
                    {
                        "enum": [
                            "skip",
                            "overwrite",
                            "rename",
                            "update_tags"
                        ],
                        "type": "string",
                        "default": "skip",
                        "example": "\"skip\"",
                        "description": "How to handle existing files",
                        "name": "conflict_policy",
                        "in": "query"
                    },
                    {
                        "type": "string",
                        "example": "\"netease\"",
                        "description": "Music platform to use for downloading",
                        "name": "platform",
                        "in": "query"
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful batch download response with individual song results and error details",
                        "schema": {
                            "$ref": "#/definitions/models.DownloadSummary"
                        }
                    },
                    "400": {
                        "description": "Bad request - missing or invalid parameters",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal server error during download processing",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        },
        "/login": {
            "post": {
                "description": "Authenticates user with provided credentials",
                "consumes": [
                    "application/json"
                ],
                "produces": [
                    "application/json"
                ],
                "tags": [
                    "auth"
                ],
                "summary": "User login",
                "parameters": [
                    {
                        "type": "string",
                        "example": "\"netease\"",
                        "description": "Music platform",
                        "name": "platform",
                        "in": "query"
                    },
                    {
                        "description": "Login request",
                        "name": "body",
                        "in": "body",
                        "required": true,
                        "schema": {
                            "$ref": "#/definitions/models.LoginRequest"
                        }
                    }
                ],
                "responses": {
                    "200": {
                        "description": "OK",
                        "schema": {
                            "$ref": "#/definitions/models.LoginResponse"
                        }
                    },
                    "400": {
                        "description": "Bad Request",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    },
                    "500": {
                        "description": "Internal Server Error",
                        "schema": {
                            "$ref": "#/definitions/models.ErrorResponse"
                        }
                    }
                }
            }
        }
    },
    "definitions": {
        "github_com_stkevintan_miko_pkg_types.Album": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                },
                "picUrl": {
                    "type": "string"
                }
            }
        },
        "github_com_stkevintan_miko_pkg_types.Artist": {
            "type": "object",
            "properties": {
                "id": {
                    "type": "integer"
                },
                "name": {
                    "type": "string"
                }
            }
        },
        "models.DownloadSummary": {
            "type": "object",
            "properties": {
                "details": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/types.DownloadResult"
                    }
                },
                "summary": {
                    "type": "string",
                    "example": "Downloaded 8 out of 10 songs."
                }
            }
        },
        "models.ErrorResponse": {
            "description": "Error response",
            "type": "object",
            "properties": {
                "error": {
                    "type": "string",
                    "example": "Invalid input"
                }
            }
        },
        "models.LoginRequest": {
            "description": "User login request",
            "type": "object",
            "required": [
                "password",
                "uuid"
            ],
            "properties": {
                "password": {
                    "type": "string",
                    "example": "your-cookiecloud-password"
                },
                "uuid": {
                    "type": "string",
                    "example": "your-cookiecloud-uuid"
                }
            }
        },
        "models.LoginResponse": {
            "description": "User login response",
            "type": "object",
            "properties": {
                "message": {
                    "type": "string",
                    "example": "Login successful"
                },
                "success": {
                    "type": "boolean",
                    "example": true
                },
                "user_id": {
                    "type": "integer",
                    "example": 123456789
                },
                "username": {
                    "type": "string",
                    "example": "john_doe"
                }
            }
        },
        "types.DownloadResult": {
            "type": "object",
            "properties": {
                "data": {
                    "$ref": "#/definitions/types.DownloadedMusic"
                },
                "error": {}
            }
        },
        "types.DownloadedMusic": {
            "type": "object",
            "properties": {
                "album": {
                    "$ref": "#/definitions/github_com_stkevintan_miko_pkg_types.Album"
                },
                "artist": {
                    "type": "array",
                    "items": {
                        "$ref": "#/definitions/github_com_stkevintan_miko_pkg_types.Artist"
                    }
                },
                "filePath": {
                    "type": "string"
                },
                "id": {
                    "type": "integer"
                },
                "lyrics": {
                    "type": "string"
                },
                "name": {
                    "type": "string"
                },
                "quality": {
                    "type": "string"
                },
                "size": {
                    "type": "integer"
                },
                "time": {
                    "type": "integer"
                },
                "trackNumber": {
                    "type": "string"
                },
                "type": {
                    "type": "string"
                },
                "url": {
                    "type": "string"
                }
            }
        }
    }
}